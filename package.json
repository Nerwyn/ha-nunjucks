{
	"name": "ha-nunjucks",
	"version": "1.2.4",
	"description": "Wrapper for nunjucks for use with Home Assistant frontend custom components to render templates",
	"main": "./dist/index.js",
	"scripts": {
		"test": "jest --silent=false",
		"build": "tsc",
		"prelint": "tsc --noemit",
		"lint": "eslint ./src --fix --ext .ts --config ./.eslintrc.js"
	},
	"lint-staged": {
		"*": "prettier -w"
	},
	"author": "Nerwyn",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Nerwyn/ha-nunjucks.git"
	},
	"bugs": {
		"url": "https://github.com/Nerwyn/ha-nunjucks/issues"
	},
	"homepage": "https://github.com/Nerwyn/ha-nunjucks#readme",
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"files": [
		"./dist",
		"README.md"
	],
	"dependencies": {
		"@types/nunjucks": "latest",
		"custom-card-helpers": "latest",
		"nunjucks": "latest"
	},
	"devDependencies": {
		"@types/jest": "latest",
		"@typescript-eslint/eslint-plugin": "latest",
		"@typescript-eslint/parser": "latest",
		"eslint": "latest",
		"prettier": "latest",
		"lint-staged": "latest",
		"ts-jest": "latest",
		"ts-loader": "latest",
		"typescript": "latest"
	}
}
